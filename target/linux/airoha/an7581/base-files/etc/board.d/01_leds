. /lib/functions/uci-defaults.sh

board_config_update

board=$(board_name)

case "$board" in
airoha,an7581-evb)
	ucidef_set_led_usbport "usb1" "USB 1" "green:usb-1" "usb1-port1" "usb2-port1"
	ucidef_set_led_usbport "usb2" "USB 2" "green:usb-2" "usb3-port1" "usb4-port1"
	;;
nokia,xg-040g-md)
	ucidef_set_led_netdev "lan2-led" "LAN2 LED" "lan2" "lan2" "link tx rx"
	ucidef_set_led_netdev "lan3-led" "LAN3 LED" "lan3" "lan3" "link tx rx"
	ucidef_set_led_netdev "lan4-led" "LAN4 LED" "lan4" "lan4" "link tx rx"
	;;
esac

board_config_flush

exit 0
